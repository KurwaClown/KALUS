<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="LOG_FONT">#121212</SolidColorBrush>

    <SolidColorBrush x:Key="LOG_INFO">#BF044BCC</SolidColorBrush>
    <SolidColorBrush x:Key="LOG_INFO_BACKGROUND">#30044BCC</SolidColorBrush>

    <SolidColorBrush x:Key="LOG_WARN">#FFEC46</SolidColorBrush>
    <SolidColorBrush x:Key="LOG_WARN_BACKGROUND">#FAFAD2</SolidColorBrush>

    <SolidColorBrush x:Key="LOG_ERROR">#DC041C</SolidColorBrush>
    <SolidColorBrush x:Key="LOG_ERROR_BACKGROUND">#FFE6E6</SolidColorBrush>

    <SolidColorBrush x:Key="LOG_DEFAULT">#000000</SolidColorBrush>
    <SolidColorBrush x:Key="LOG_DEFAULT_BACKGROUND">#FFFFFF</SolidColorBrush>

    <SolidColorBrush x:Key="STATE_FONT">#121212</SolidColorBrush>

    <SolidColorBrush x:Key="STATE_NOCLIENT">#DC244C</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_NONE">#CCCCCC</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_LOBBY">#24DCD7</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_MATCHMAKING">#FF8E1E</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_READYCHECK">#15BB15</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_CHAMPSELECT">#FEE147</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_GAMESTART">#8B2FFC</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_DEFAULT">#800000</SolidColorBrush>
</ResourceDictionary>