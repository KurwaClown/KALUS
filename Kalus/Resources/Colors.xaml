<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="LOG_FONT">#262626</SolidColorBrush>
    
    <SolidColorBrush x:Key="LOG_INFO">#3139FF</SolidColorBrush>
    <SolidColorBrush x:Key="LOG_INFO_BACKGROUND">#F0F8FF</SolidColorBrush>
    
    <SolidColorBrush x:Key="LOG_WARN">#FAF520</SolidColorBrush>
    <SolidColorBrush x:Key="LOG_WARN_BACKGROUND">#FAFAD2</SolidColorBrush>
    
    <SolidColorBrush x:Key="LOG_ERROR">#DC041C</SolidColorBrush>
    <SolidColorBrush x:Key="LOG_ERROR_BACKGROUND">#FFE6E6</SolidColorBrush>
    
    <SolidColorBrush x:Key="LOG_DEFAULT">#000000</SolidColorBrush>
    <SolidColorBrush x:Key="LOG_DEFAULT_BACKGROUND">#FFFFFF</SolidColorBrush>

    <SolidColorBrush x:Key="STATE_FONT">#262626</SolidColorBrush>

    <SolidColorBrush x:Key="STATE_NOCLIENT">#DC244C</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_ARAM">#90EE90</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_BLIND">#87CEFA</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_DRAFT">#87CEFA</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_CHAMPSELECT">#D3D3D3</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_LOBBY">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_READYCHECK">#15BB15</SolidColorBrush>
    <SolidColorBrush x:Key="STATE_DEFAULT">#FFFFFF</SolidColorBrush>
</ResourceDictionary>